{
  "doctype-first": false,
  "doctype-html5": true,
  "html-lang-require": true,
  "head-script-disabled": true,
  "style-disabled": true,
  "script-disabled": true,
  "title-require": true,
  "attr-lowercase": ["*ngFor","[(ngModel)]", "(ngSubmit)", "[routerLink]", "routerLink", "routerLinkActive", "*ngIf", "#loggedOut",
    "[ngClass]","viewBox","#displayName", "#using2FA", "#matchingPassword", "stackItemSize"],
  "attr-no-duplication": true,
  "attr-no-unnecessary-whitespace": true,
